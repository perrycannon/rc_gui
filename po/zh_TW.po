#: ../data/rc_gui.ui.h:2
msgid "Filesystem:"
msgstr "檔案系統："

#: ../data/rc_gui.ui.h:3
msgid "Expand _Filesystem"
msgstr "擴展檔案系統(_F)"

#: ../data/rc_gui.ui.h:4
msgid "Ensure that all of the SD card storage is available to the OS"
msgstr "確保整個 SD 記憶卡的儲存空間都可以給作業系統使用"

#: ../data/rc_gui.ui.h:5
msgid "Password:"
msgstr "密碼："

#: ../data/rc_gui.ui.h:6
msgid "Change _Password..."
msgstr "變更密碼... (_P)"

#: ../data/rc_gui.ui.h:7
msgid "Change password for the default user (pi)"
msgstr "為預設的使用者 (pi) 變更密碼"

#: ../data/rc_gui.ui.h:8
msgid "Hostname:"
msgstr "主機名稱："

#: ../data/rc_gui.ui.h:9
msgid "Set the name of your Pi on a local network"
msgstr "設定您的 Pi 在本機網路上的名稱"

#: ../data/rc_gui.ui.h:10
msgid "Boot:"
msgstr "開機："

#: ../data/rc_gui.ui.h:11
msgid "To Desktop"
msgstr "到桌面"

#: ../data/rc_gui.ui.h:12
msgid "Launch the desktop environment automatically on boot"
msgstr "在開機時自動進入桌面環境"

#: ../data/rc_gui.ui.h:13
msgid "To CLI"
msgstr "到命令列介面"

#: ../data/rc_gui.ui.h:14
msgid "Boot into a command line shell rather than the desktop"
msgstr "開機時進入命令列殼層而非桌面環境"

#: ../data/rc_gui.ui.h:15
msgid "Auto login:"
msgstr "自動登入："

#: ../data/rc_gui.ui.h:16
msgid "Login as user 'pi'"
msgstr "登入為使用者「pi」"

#: ../data/rc_gui.ui.h:17
msgid "Automatically log in the default user (pi)"
msgstr "自動登入到預設的使用者 (pi)"

#: ../data/rc_gui.ui.h:18
msgid "Network at Boot:"
msgstr "開機時的網路："

#: ../data/rc_gui.ui.h:19
msgid "Wait for network"
msgstr "等待網路"

#: ../data/rc_gui.ui.h:20
msgid "Do not complete boot until network connection is established"
msgstr "在網路連線建立前不要完成開機"

#: ../data/rc_gui.ui.h:21
msgid "Overscan:"
msgstr "過掃描："

#: ../data/rc_gui.ui.h:22
msgid "Enable"
msgstr "啟用"

#: ../data/rc_gui.ui.h:23
msgid "Enable overscan to show the edges of the screen"
msgstr "啟用過掃描以顯示螢幕的邊緣"

#: ../data/rc_gui.ui.h:24
msgid "Disable"
msgstr "停用"

#: ../data/rc_gui.ui.h:25
msgid "Disable overscan to hide black outline on screen"
msgstr "停用過掃描以隱藏螢幕上的黑色外框"

#: ../data/rc_gui.ui.h:26
msgid "Rastrack:"
msgstr "Rastrack："

#: ../data/rc_gui.ui.h:27
msgid "Add to _Rastrack..."
msgstr "加入 Rastrack... (_R)"

#: ../data/rc_gui.ui.h:28
msgid "Add this Pi to the online Raspberry Pi map"
msgstr "將這臺 Pi 加入到線上的 Raspberry Pi 地圖中"

#: ../data/rc_gui.ui.h:29
msgid "System"
msgstr "系統"

#: ../data/rc_gui.ui.h:30
msgid "Camera:"
msgstr "攝影機："

#: ../data/rc_gui.ui.h:31
msgid "Enable the Raspberry Pi Camera Board"
msgstr "啟用 Raspberry Pi 攝影機板"

#: ../data/rc_gui.ui.h:32
msgid "Disable the Raspberry Pi Camera Board"
msgstr "停用 Raspberry Pi 攝影機板"

#: ../data/rc_gui.ui.h:33
msgid "SSH:"
msgstr "SSH："

#: ../data/rc_gui.ui.h:34
msgid "Enable remote access to this Pi via SSH"
msgstr "啟用經由 SSH 遠端存取這臺 Pi"

#: ../data/rc_gui.ui.h:35
msgid "Disable remote access to this Pi via SSH"
msgstr "停用經由 SSH 遠端存取這臺 Pi"

#: ../data/rc_gui.ui.h:36
msgid "SPI:"
msgstr "SPI："

#: ../data/rc_gui.ui.h:37
msgid "Enable the automatic loading of the SPI kernel module"
msgstr "啟用自動載入 SPI 核心模組"

#: ../data/rc_gui.ui.h:38
msgid "Disable the automatic loading of the SPI kernel module"
msgstr "停用自動載入 SPI 核心模組"

#: ../data/rc_gui.ui.h:39
msgid "I2C:"
msgstr "I2C："

#: ../data/rc_gui.ui.h:40
msgid "Enable the automatic loading of the I2C kernel module"
msgstr "啟用自動載入 I2C 核心模組"

#: ../data/rc_gui.ui.h:41
msgid "Disable the automatic loading of the I2C kernel module"
msgstr "停用自動載入 I2C 核心模組"

#: ../data/rc_gui.ui.h:42
msgid "Serial:"
msgstr "串列："

#: ../data/rc_gui.ui.h:43
msgid "Enable shell and kernel messages on the serial connection"
msgstr "在串列連線中啟用殼層與核心訊息"

#: ../data/rc_gui.ui.h:44
msgid "Disable shell and kernel messages on the serial connection"
msgstr "在串列連線中停用殼層與核心訊息"

#: ../data/rc_gui.ui.h:45
msgid "1-Wire:"
msgstr "1-Wire："

#: ../data/rc_gui.ui.h:46
msgid "Enable 1-wire peripheral interface"
msgstr "啟用 1-wire 外部介面"

#: ../data/rc_gui.ui.h:47
msgid "Disable 1-wire peripheral interface"
msgstr "停用 1-wire 外部介面"

#: ../data/rc_gui.ui.h:48
msgid "Remote GPIO:"
msgstr "遠端 GPIO："

#: ../data/rc_gui.ui.h:49
msgid "Enable remote access to GPIO pins"
msgstr "啟用遠端存取 GPIO 針腳"

#: ../data/rc_gui.ui.h:50
msgid "Disable remote access to GPIO pins"
msgstr "停用遠端存取 GPIO 針腳"

#: ../data/rc_gui.ui.h:51
msgid "Interfaces"
msgstr "介面"

#: ../data/rc_gui.ui.h:52
msgid "O_verclock:"
msgstr "超頻(_V)："

#: ../data/rc_gui.ui.h:53
msgid "Set the level of overclock to control performance"
msgstr "設定超頻等級以控制效能"

#: ../data/rc_gui.ui.h:54
msgid "None (700MHz)"
msgstr "無 (700MHz)"

#: ../data/rc_gui.ui.h:55
msgid "Modest (800MHz)"
msgstr "適量 (800MHz)"

#: ../data/rc_gui.ui.h:56
msgid "Medium (900MHz)"
msgstr "中等 (900MHz)"

#: ../data/rc_gui.ui.h:57
msgid "High (950MHz)"
msgstr "高 (950MHz)"

#: ../data/rc_gui.ui.h:58
msgid "Turbo (1000MHz)"
msgstr "渦輪加速 (1000MHz)"

#: ../data/rc_gui.ui.h:59
msgid "None (900MHz)"
msgstr "無 (900MHz)"

#: ../data/rc_gui.ui.h:60
msgid "High (1000MHz)"
msgstr "高 (1000MHz)"

#: ../data/rc_gui.ui.h:61
msgid "Not available"
msgstr "不可用"

#: ../data/rc_gui.ui.h:62
msgid "_GPU Memory:"
msgstr "GPU 記憶體(_G)："

#: ../data/rc_gui.ui.h:63
msgid "Enter the amount of memory available to the GPU"
msgstr "輸入要提供給 GPU 的記憶體大小"

#: ../data/rc_gui.ui.h:64
msgid "Performance"
msgstr "效能"

#: ../data/rc_gui.ui.h:65
msgid "Locale:"
msgstr "語系："

#: ../data/rc_gui.ui.h:66
msgid "Set _Locale..."
msgstr "設定語系... (_L)"

#: ../data/rc_gui.ui.h:67
msgid "Set internationalisation settings for units etc."
msgstr "為單位等事物設定國際化設定"

#: ../data/rc_gui.ui.h:68
msgid "Timezone:"
msgstr "時區："

#: ../data/rc_gui.ui.h:69
msgid "Set _Timezone..."
msgstr "設定時區... (_T)"

#: ../data/rc_gui.ui.h:70
msgid "Set the timezone to be used by the internal clock"
msgstr "設定要用於內部時鐘的時區"

#: ../data/rc_gui.ui.h:71
msgid "Keyboard:"
msgstr "鍵盤："

#: ../data/rc_gui.ui.h:72
msgid "Set _Keyboard..."
msgstr "設定鍵盤... (_K)"

#: ../data/rc_gui.ui.h:73
msgid "Set international keyboard options"
msgstr "設定國際鍵盤選項"

#: ../data/rc_gui.ui.h:74
msgid "WiFi Country:"
msgstr "WiFi 國家："

#: ../data/rc_gui.ui.h:75
msgid "Set _WiFi Country..."
msgstr "設定 WiFi 國家... (_W)"

#: ../data/rc_gui.ui.h:76
msgid "Set WiFi country code"
msgstr "設定 WiFi 國碼"

#: ../data/rc_gui.ui.h:77
msgid "Localisation"
msgstr "在地化"

#: ../data/rc_gui.ui.h:78
msgid ""
"The Raspberry Pi Configuration application can only modify a standard "
"configuration.\n"
"\n"
"Your configuration appears to have been modified by other tools, and so this "
"application cannot be used on your system.\n"
"\n"
"In order to use this application, you need to have the latest firmware "
"installed, Device Tree enabled, the default \"pi\" user set up and the "
"lightdm application installed. "
msgstr ""
"Raspberry Pi 設定應用程式只能修改標準的 設定。\n"
"\n"
"您的設定似乎已被其他工具修改過了，所以 本應用程式無法在您的系統上使用。\n"
"\n"
"為了使用本應用程式，您需要安裝最新的韌體， 並啟用裝置樹，預設的「pi」使用者設"
"定好並 安裝好 lightdm。 "

#: ../data/rc_gui.ui.h:83
msgid "Filesystem Expanded"
msgstr "檔案系統已擴展"

#: ../data/rc_gui.ui.h:84
msgid ""
"The filesystem has been expanded.\n"
"\n"
"The new space will not be available until you reboot your Raspberry Pi."
msgstr ""
"檔案系統已經被擴展了。\n"
"\n"
"在您重新啟動您的 Raspberry Pi 後，新的空間才可用。"

